<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="HQL" injector-id="groovy">
      <display-name>Grails HQL injection</display-name>
      <place disabled="true"><![CDATA[groovyElement().methodCallParameter(0, grLightMethod("Gorm:DomainDescriptor:DynamicMethod").withName(string().oneOf("find", "findAll", "executeQuery", "executeUpdate")))]]></place>
    </injection>
    <injection language="JAVA" injector-id="java">
      <display-name>PrintStream.println (java.io)</display-name>
      <place><![CDATA[psiParameter().ofMethod(0, psiMethod().withName("println").withParameters("java.lang.String").definedInClass("java.io.PrintStream"))]]></place>
    </injection>
  </component>
</application>